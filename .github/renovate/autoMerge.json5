{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true,
      "automergeSchedule": "after 1am and before 6am",
      "requiredStatusChecks": null,
      "ignoreTests": true
    }
  ]
}
